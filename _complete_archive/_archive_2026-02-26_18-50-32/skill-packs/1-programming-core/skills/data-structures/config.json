    "keywords": [
      "data structure",
      "array",
      "linked list",
      "tree",
      "graph",
      "stack",
      "queue",
      "hash map",
      "heap"
    ],
    "patterns": [
      "implement.*data.*structure",
      "choose.*data.*structure",
