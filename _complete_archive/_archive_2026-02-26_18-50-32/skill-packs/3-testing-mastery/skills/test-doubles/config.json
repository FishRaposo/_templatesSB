{
  "agent_support": {
    "claude": {},
    "roo": {},
    "cascade": {},
    "generic": {}
  },
  "triggers": {
    "keywords": [
      "mock",
      "stub",
      "fake",
      "spy",
      "test double",
      "mocking",
      "jest mock",
      "unittest.mock",
      "isolate test"
    ],
    "patterns": [
      "mock.*(api|database|service)",
      "create.*(stub|fake|mock)",
      "isolate.*test.*depend"
    ]
  },
  "requirements": {
    "tools": [],
    "permissions": ["file_read", "file_write"]
  },
  "examples": {
    "simple": [
      "Create a mock for this API",
      "Mock the database for testing",
      "Use test doubles to isolate this test"
    ],
    "complex": [
      "Set up mocks for all external dependencies",
      "Create a fake database for testing",
      "Mock and verify HTTP requests in tests"
    ]
  }
}
